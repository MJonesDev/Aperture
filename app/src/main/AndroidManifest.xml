<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marlonjones.projectevo"
    android:versionCode="1"
    android:versionName="1.0" >



    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".ApertureBasing"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="${appName}"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.marlonjones.projectevo.activity.ActivityHome"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.marlonjones.projectevo.activity.AlbumActivity"
            android:label="@string/app_name"
            android:theme="@style/AppThemeNew" >
        </activity>
        <activity
            android:name="com.marlonjones.projectevo.activity.PhotoPreviewActivity"
            android:label="@string/app_name"
            android:theme="@style/AppThemeNew" >
        </activity>
    </application>

</manifest>